<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">

        <shape android:shape="rectangle">

            <gradient
                android:angle="90"
                android:endColor="#f3ffe1"
                android:startColor="#c0dea8"
                android:type="linear" />

            <corners android:radius="@dimen/corner_radius" />

            <stroke
                android:width="6px"
                android:color="#22831b" />

        </shape>
    </item>
</layer-list>